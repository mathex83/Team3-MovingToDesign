<Window x:Class="FISK.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FISK"
        xmlns:uc="clr-namespace:FISK"
        mc:Ignorable="d"
        Title="FISK" Height="812" Width="375" Background="#CECECE" Margin="0" WindowStartupLocation="CenterScreen">
    <Grid Margin="0">
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="600"/>
            <RowDefinition Height="50"/>
            <RowDefinition Height="50"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Text="FISK" Style="{StaticResource TextBlockStyle}" FontSize="36"/>
        <TextBlock Text="Frivillige I Skive Kommune" Style="{StaticResource TextBlockStyle}" Grid.Row="1"/>
        <Frame x:Name="PageFrame" Grid.Row="2" NavigationUIVisibility="Hidden"
               x:FieldModifier="public"/>
        <uc:BottomBar Grid.Row="4"/>
    </Grid>
</Window>
