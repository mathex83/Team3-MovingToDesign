<UserControl x:Class="FISK_Prototype.View.Usercontrols.SecondBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FISK_Prototype.View.Usercontrols"
             mc:Ignorable="d" 
             Style="{StaticResource UCStyle1}">
    <Grid>
        <Button Style="{StaticResource UCButtonStyle}" Content="Login" Width="40" Margin="0,0,300,0"
                Command="{Binding SetPageContentCommand}"
                CommandParameter="LoginSubPage1"/>
        <Button Style="{StaticResource UCButtonStyle}" Content="Opret bruger" Width="80" Margin="0,0,170,0"
                Command="{Binding SetPageContentCommand}"
                CommandParameter="LoginSubPage2"/>
    </Grid>
</UserControl>
